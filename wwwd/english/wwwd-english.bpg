#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = wwwd.exe wwwd.JPN
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

wwwd.exe: wwwd.dpr
  $(DCC)

wwwd.JPN: JPN\wwwd.dpr
  $(DCC)


